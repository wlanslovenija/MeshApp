#!/system/bin/sh
	
wpa_cli	-i tiwlan0 -p /data/misc/wifi set_network 2 mode 1
wpa_cli -i tiwlan0 -p /data/misc/wifi set_network 2 ssid "open.wlan-lj.net"
wpa_cli -i tiwlan0 -p /data/misc/wifi set_network 2 bssid 02:ca:ff:ee:ba:be
wpa_cli -i tiwlan0 -p /data/misc/wifi set_network 2 frequency 2447
wpa_cli -i tiwlan0 -p /data/misc/wifi set_network 2 key_mgmt NONE
wpa_cli -i tiwlan0 -p /data/misc/wifi enable_network 2
wpa_cli -i tiwlan0 -p /data/misc/wifi ap_scan 2
wpa_cli -i tiwlan0 -p /data/misc/wifi select_network 2
	
set -x
sleep 5
ifconfig tiwlan0 104.198.65.66 netmask 255.0.0.0 broadcast 104.255.255.255